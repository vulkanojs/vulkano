{
  "name": "vulkano",
  "version": "2.0.0",
  "description": "A MVC framework using Express 4",
  "license": "MIT",
  "engines": {
    "node": "^20"
  },
  "main": "vulkano.js",
  "scripts": {
    "dev": "node ./node_modules/concurrently/dist/bin/concurrently -n VULKANO -c green,cyan,magenta './node_modules/nodemon/bin/nodemon.js --inspect'",
    "start": "node vulkano.js",
    "deploy:staging": "pm2 deploy staging",
    "deploy:server": "pm2 deploy production"
  },
  "dependencies": {
    "@vulkano/core": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulkanojs/vulkano.git"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "devDependencies": {

  }
}
