{
  "name": "vulkano",
  "version": "1.18.0",
  "description": "A MVC framework using Express 4",
  "license": "MIT",
  "engines": {
    "node": "^18 || ^20"
  },
  "main": "app.js",
  "scripts": {
    "dev": "node ./node_modules/concurrently/dist/bin/concurrently -n VULKANO,WEPBACK,GULP -c green,cyan,magenta './node_modules/nodemon/bin/nodemon.js --inspect' 'npm run webpack' 'npm run gulp'",
    "start": "node app.js",
    "postinstall": "npm run build",
    "gulp": "gulp --gulpfile gulpfile.app.js",
    "webpack": "webpack --config webpack.app.js -w",
    "build": "NODE_ENV=production webpack --config webpack.app.js && NODE_ENV=production gulp --gulpfile gulpfile.app.js",
    "deploy:staging": "pm2 deploy staging",
    "deploy:server": "pm2 deploy production"
  },
